                                <div class="card">
                                    <div class="card-content main-info" style="background-color:#5B76AD;text-align:center;">
                                        <div class="innerContent">
                                            <div style="color:white;font-size:30px">240</div>
                                            <div style="color:white;font-size:12px">Implementation Partnership</div>
                                        </div>


                                    </div>
                                </div>
                            </div>

                            @*Institutional Partnership*@
