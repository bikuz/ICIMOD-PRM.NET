                                <div class="card">
                                    <div class="card-content main-info" style="text-align:center;">
                                        <div class="innerContent">
                                            <div style="color:#23B7E5;font-size:30px">257</div>
                                            <div style="font-size:12px">Institutional Collaboration</div>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    @*Highlights*@
                    <div class="col-md-8" style="padding-right:5px">
