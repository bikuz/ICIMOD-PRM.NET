                                <div class="card">
                                    <div class="card-content main-info" style="text-align:center;">
                                        <div class="innerContent">
                                            <div style="color:#5B76AD;font-size:30px">112</div>
                                            <div style="font-size:12px">Strategic Partnership</div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            @*Operational Partnership*@
                            <div class="col-md-6" style="padding-left:10px">
                                <div class="card">
                                    <div class="card-content main-info" style="background-color:#23B7E5;text-align:center;">
                                        <div class="innerContent">
                                            <div style="color:white;font-size:30px">176</div>
                                            <div style="color:white;font-size:12px">Operational Partnership</div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            @*Implementation Partnership*@
