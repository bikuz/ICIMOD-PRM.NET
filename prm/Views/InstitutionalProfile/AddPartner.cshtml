@{
    ViewBag.Title = "Add Partner";
}
 
<div class="row">

    <div class="container-fluid">
        <!--Data Card Container Row-->
        <div class="row" style="margin:10px;">

            <div class="col-md-12">
                <div class="card" id="card-partner-main">
                    <div class="card-header">
                        <!--Card Title-->
                        <span class="card-title">Add Potential Partner</span>
                        <span class="cardButton"><a href="#" class="cardCollapseLink" id="card-partner"><i class="fa fa-chevron-circle-down fa-lg fa-bold" style="color:#ffffff;" aria-hidden="true"></i></a></span>
                        <!--Card-Header Ends-->
                    </div>
                    <h3 style="text-align:center">Institutional Information</h3>
                    <div class="card-content">
                        <ul class="nav nav-tabs nav-justified tabs-modified">
                            <li role="presentation" class="active"><a data-toggle="tab" href="#ContactInfo">Contact Information</a></li>
                            <li role="presentation"><a data-toggle="tab" href="#orgInfo">Organizational Information</a></li>
                        </ul>

                        <div class="tab-content">
                            <div class="guide-info">
                                You will be required to enter the detailed information about the partner institution in this form.
                                Information relating to the establishment, structure and contact information will be required in order to fill this form.
                                Proceeding forms cannot be processed without completing this form.
                                You will spend roughly 10 minutes to complete this form.
                            </div>

                            <div id="ContactInfo" class="tab-pane fade in active">
                                <form class="form-horizontal" role="form" style="margin-top:20px;">
                                    <div class="row">

                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="control-label col-md-4">Organization Name</label>
                                                <div class="col-md-8">
                                                    <input type="text" class="form-control" id="OrganizationName" />
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <label class="control-label col-md-4">Acronym</label>
                                                <div class="col-md-8">
                                                    <input type="text" class="form-control" id="Acronym" />
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <label class="control-label col-md-4">Country of Registration</label>
                                                <div class="col-md-8">
                                                    <input type="text" class="form-control" id="Country" />
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <label class="control-label col-md-4">Email Address</label>
                                                <div class="col-md-8">
                                                    <input type="email" class="form-control" id="Email" />
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <label class="control-label col-md-4">Office Location</label>
                                                <div class="col-md-8">
                                                    <input type="text" class="form-control" id="Street" />
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <label class="control-label col-md-4">Phone Number</label>
                                                <div class="col-md-8">
                                                    <input type="text" class="form-control" id="Phone1" placeholder="Primary" />
                                                </div>


                                            </div>
                                            <div class="form-group">
                                                <label class="control-label col-md-4"></label>
                                                <div class="col-md-8">
                                                    <input type="text" class="form-control" id="Phone2" placeholder="Secondary" />
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="control-label col-md-4">Partner Code</label>
                                                <div class="col-md-8">
                                                    <input type="text" class="form-control" id="PartnerCode" />
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <label class="control-label col-md-4">Postal Address</label>
                                                <div class="col-md-8">
                                                    <input type="text" class="form-control" id="PostalCode" />
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <label class="control-label col-md-4">Skype ID</label>
                                                <div class="col-md-8">
                                                    <input type="text" class="form-control" id="SkypeID" />
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <label class="control-label col-md-4">Website</label>
                                                <div class="col-md-8">
                                                    <input type="text" class="form-control" id="Website" />
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <label class="control-label col-md-4">Upload Logo</label>
                                                <div class="col-md-8">
                                                    <input type="file" style="padding: 6px 12px;" id="file" />
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <label class="control-label col-md-4">Fax</label>
                                                <div class="col-md-8">
                                                    <input type="text" class="form-control" id="Fax1" placeholder="Primary" />
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="control-label col-md-4"></label>
                                                <div class="col-md-8">
                                                    <input type="text" class="form-control" id="Fax2" placeholder="Secondary" />
                                                </div>
                                            </div>
                                        </div>

                                    </div>

                                    <div class="row">
                                        <div class="col-md-12"><div class="horizontal-line"></div></div>
                                        <label class="control-label col-md-12" style="text-align:center;display:block;margin-bottom:10px;"> Head of Organization</label>
                                        <div class="col-md-3">
                                            <input type="text" class="form-control" id="AuthorisedName" placeholder="Name" />
                                        </div>
                                        <div class="col-md-2">
                                            <input type="text" class="form-control" id="AuthorisedJob" placeholder="Designation" />
                                        </div>
                                        <div class="col-md-3">
                                            <input type="text" class="form-control" id="AuthorisedPhone" placeholder="Telephone" />
                                        </div>
                                        <div class="col-md-2">
                                            <input type="email" class="form-control" id="AuthorisedEmail" placeholder="Email" />
                                        </div>
                                        <div class="col-md-2">
                                            <input type="text" class="form-control" id="AuthorisedSkypeID" placeholder="Skype ID" />
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-12"><div class="horizontal-line"></div></div>
                                        <label class="control-label col-md-12" style="text-align:center;display:block;margin-bottom:10px;">Contact Persons</label>
                                        <div class="col-md-3">
                                            <input type="text" class="form-control" id="primaryContactName" placeholder="Primary Contact Name" />
                                        </div>
                                        <div class="col-md-2">
                                            <input type="text" class="form-control" id="primaryContactDesignation" placeholder="Designation" />
                                        </div>
                                        <div class="col-md-3">
                                            <input type="text" class="form-control" id="primaryContactTelephone" placeholder="Telephone" />
                                        </div>
                                        <div class="col-md-2">
                                            <input type="text" class="form-control" id="primaryContactEmail" placeholder="Email" />
                                        </div>
                                        <div class="col-md-2">
                                            <input type="text" class="form-control" id="primaryContactSkypeID" placeholder="Skype ID" />
                                        </div>

                                        <label class="control-label col-md-12" style="text-align:center;display:block;margin-bottom:10px;"></label>
                                        <div class="col-md-3">
                                            <input type="text" class="form-control" id="secondaryContactName" placeholder="Name" />
                                        </div>
                                        <div class="col-md-2">
                                            <input type="text" class="form-control" id="secondaryContactDesignation" placeholder="Designation" />
                                        </div>
                                        <div class="col-md-3">
                                            <input type="text" class="form-control" id="secondaryContactTelephone" placeholder="Telephone" />
                                        </div>
                                        <div class="col-md-2">
                                            <input type="text" class="form-control" id="secondaryContactEmail" placeholder="Email" />
                                        </div>
                                        <div class="col-md-2">
                                            <input type="text" class="form-control" id="secondaryContactSkypeID" placeholder="Skype ID" />
                                        </div>

                                        <div class="col-md-12"><div class="horizontal-line"></div></div>

                                        <div style="float:right;margin-right:30px;">
                                            <button type="submit" class="btn btn-primary">Save and Continue</button>
                                            <button type="submit" class="btn btn-primary">Save and Exit</button>
                                            <button type="submit" class="btn btn-primary">Cancel</button>
                                        </div>
                                    </div>

                                </form>
                            </div>

                            <div id="orgInfo" class="tab-pane fade in">
                                <form role="form" style="margin-top:20px;">
                                    <div class="row">
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label for="estdate">Established Date</label>
                                                <input type="text" class="form-control" id="estdate">
                                            </div>
                                        </div>

                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label for="natureInstitution">Nature of Institution</label>
                                                <input type="text" class="form-control" id="natureInstitution">
                                            </div>
                                        </div>

                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label for="workingAreas">Working Areas</label>
                                                <input type="text" class="form-control" id="workingAreas">
                                            </div>
                                        </div>
                                    </div>


                                    <div class="row">
                                        <div class="col-md-12"><div class="horizontal-line"></div></div>
                                        <label class="control-label col-md-12" style="text-align:center;display:block;margin-bottom:10px;">Other Nature of Institution (If Any)</label>
                                        <div class="col-md-12">
                                            <div class="mce-holder"></div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-12"><div class="horizontal-line"></div></div>
                                        <label class="control-label col-md-12" style="text-align:center;display:block;margin-bottom:10px;">Other Working Areas (If Any)</label>
                                        <div class="col-md-12">
                                            <div class="mce-holder"></div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-12"><div class="horizontal-line"></div></div>
                                        <label class="control-label col-md-12" style="text-align:center;display:block;margin-bottom:10px;">Organizational Objective</label>
                                        <div class="col-md-12">
                                            <div class="mce-holder"></div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-12"><div class="horizontal-line"></div></div>
                                        <label class="control-label col-md-12" style="text-align:center;display:block;margin-bottom:10px;">Organizational Brief</label>
                                        <div class="col-md-12">
                                            <div class="mce-holder"></div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-12"><div class="horizontal-line"></div></div>
                                        <label class="control-label col-md-12" style="text-align:center;display:block;margin-bottom:10px;">Core Competencies and Expertise</label>
                                        <div class="col-md-12">
                                            <div class="mce-holder"></div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-12"><div class="horizontal-line"></div></div>
                                        <div class="col-md-6">
                                            <div class="form-group" style="text-align:center">
                                                <label for="estdate">Experience of working in Hindu Kush Himalaya Region?</label>
                                                <div class="radio">
                                                    <label><input type="radio" name="workHKH">Yes</label>
                                                </div>
                                                <div class="radio">
                                                    <label><input type="radio" name="workHKH">No</label>
                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group" style="text-align:center">
                                                <label for="estdate">Experience of working with ICIMOD?</label>
                                                <div class="radio">
                                                    <label><input type="radio" name="workICIMOD">Yes</label>
                                                </div>
                                                <div class="radio">
                                                    <label><input type="radio" name="workICIMOD">No</label>
                                                </div>

                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-12"><div class="horizontal-line"></div></div>
                                        <label class="control-label col-md-12" style="text-align:center;display:block;margin-bottom:10px;">Operating Budget</label>
                                        <div class="col-md-3"></div>
                                        <div class="col-md-3">
                                            <div class="form-group" style="text-align:center">
                                                <label for="estdate">Year</label>
                                                <div class="radio">
                                                    <input type="text" class="form-control" id="year1" />
                                                </div>
                                                <div class="radio">
                                                    <input type="text" class="form-control" id="year2" />
                                                </div>
                                                <div class="radio">
                                                    <input type="text" class="form-control" id="year3" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group" style="text-align:center">
                                                <label for="estdate">Amount</label>
                                                <div class="radio">
                                                    <input type="text" class="form-control" id="amount1" />
                                                </div>
                                                <div class="radio">
                                                    <input type="text" class="form-control" id="amount2" />
                                                </div>
                                                <div class="radio">
                                                    <input type="text" class="form-control" id="amount3" />
                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-md-3"></div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-12"><div class="horizontal-line"></div></div>
                                        <div class="col-md-3"></div>
                                        <div class="col-md-3">
                                            <div class="form-group" style="text-align:center">
                                                <label for="estdate">Number of full time employees</label>
                                                <div class="radio">
                                                    <input type="range" min="1" max="4" id="RangeEmployee" name="RangeEmployee">
                                                    <span class="rangedef">1-25</span>
                                                    <span class="rangedef">26-75</span>
                                                    <span class="rangedef">76-100</span>
                                                    <span class="rangedef">100+</span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group" style="text-align:center">
                                                <label for="estdate">Female %</label>
                                                <div class="radio">
                                                    <input type="text" class="form-control" id="femalePercent" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-3"></div>
                                    </div>


                                    <div class="row">
                                        <div class="col-md-12"><div class="horizontal-line"></div></div>
                                        <label class="control-label col-md-12" style="text-align:center;display:block;margin-bottom:10px;">Major Achievements (Maximum Three)</label>
                                        <div class="col-md-12">
                                            <div class="mce-holder"></div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-12"><div class="horizontal-line"></div></div>
                                        <label class="control-label col-md-12" style="text-align:center;display:block;margin-bottom:10px;">Attachments</label>

                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label for="estdate">Registration Certificate</label>
                                                <div class="radio">
                                                    <input type="file" id="regCertificate" />
                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label for="estdate">Audit Report</label>
                                                <div class="radio">
                                                    <input type="file" id="auditReport1" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label for="estdate">Audit Report</label>
                                                <div class="radio">
                                                    <input type="file" id="auditReport2" />
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-md-12"><div class="horizontal-line"></div></div>

                                        <div style="float:right;margin-right:30px;">
                                            <button type="submit" class="btn btn-primary">Save and Exit</button>
                                            <button type="submit" class="btn btn-primary">Cancel</button>
                                        </div>

                                    </div>

                                </form>
                            </div>

                            <!--tab-content Ends-->
                        </div>
                        <!--Card-content Ends-->
                    </div>


                    <!--Card Ends-->
                </div>
                <!--Column ends-->
            </div>

        </div>
    </div>
</div>
