@{
    ViewBag.Title = "Import Deliverable";
}

 
<div class="row">

    <div class="container-fluid">
        <!--Data Card Container Row-->
        <div class="row" style="margin:10px;">

            <div class="col-md-12">
                <div class="card" id="card-deliverable-main">
                    <div class="card-header">
                        <!--Card Title-->
                        <span class="card-title">Import Deliverable</span>
                        <span class="cardButton"><a href="#" class="cardCollapseLink" id="card-deliverable"><i class="fa fa-chevron-circle-down fa-lg fa-bold" style="color:#ffffff;" aria-hidden="true"></i></a></span>
                        <!--Card-Header Ends-->
                    </div>
                    <h3 style="text-align:center">Information of Deliverables</h3>
                    <div class="card-content">
                        <form  role="form" style="margin-top:20px;">
                            <div class="row">
                                <div class="col-md-5"></div>
                                <div class="col-md-3" >
                                    <div class="form-group">
                                        <label class="control-label">Upload File</label>
                                       
                                        <input type="file"  id="uploadFile" />
                                        
                                    </div>
                                </div>
                                <div class="col-md-6"></div>

                                <div class="col-md-12"><div class="horizontal-line"></div></div>

                                <div style="float:right;margin-right:30px;">
                                    <button type="submit" class="btn btn-primary">Save and Continue</button>
                                    <button type="submit" class="btn btn-primary">Cancel</button>
                                </div>

                            </div>
                        </form>

                        <!--Card-content Ends-->
                    </div>


                    <!--Card Ends-->
                </div>
                <!--Column ends-->
            </div>

        </div>
    </div>
</div>